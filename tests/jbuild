(jbuild_version 1)

(executables
  ((names (Test_parser Test_pratt_calc))
   (libraries (pratt pure fmt unix astring))
   (flags (:standard -open Pure))))

(alias
 ((name runtest)
  (package pratt)
  (deps (Test_parser.exe))
  (action (run ${<}))))

(alias
 ((name runtest)
  (package pratt)
  (deps (Test_pratt_calc.exe))
  (action (run ${<}))))

